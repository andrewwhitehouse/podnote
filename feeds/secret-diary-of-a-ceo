  <title>E65: I Won 11 World Titles Because They Said I Couldn't - Anna Hemmings MBE</title>
  <link>https://audioboom.com/posts/7775909</link>
  <enclosure url="https://audioboom.com/posts/7775909.mp3?modified=1611060847&amp;source=rss&amp;stitched=1" length="81865887" type="audio/mpeg" />
  <media:content url="https://audioboom.com/posts/7775909.mp3?modified=1611060847&amp;source=rss&amp;stitched=1" type="audio/mpeg" medium="audio" duration="5114" lang="en-GB" fileSize="81865887" />
  <itunes:image href="https://images.theabcdn.com/i/36185238.jpg" />
  <media:content url="https://images.theabcdn.com/i/36185238.jpg" type="image/jpg" medium="image" />
  <itunes:duration>5114</itunes:duration>
