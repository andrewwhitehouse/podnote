  <title>E87: The Rise, The Fall &amp; The Rebuild Of True Geordie</title>
  <link>https://audioboom.com/posts/7898459</link>
  <enclosure url="https://audioboom.com/posts/7898459.mp3?modified=1625572853&amp;source=rss&amp;stitched=1" length="98651415" type="audio/mpeg" />
  <media:content url="https://audioboom.com/posts/7898459.mp3?modified=1625572853&amp;source=rss&amp;stitched=1" type="audio/mpeg" medium="audio" duration="6163" lang="en-GB" fileSize="98651415" />
  <itunes:image href="https://images.theabcdn.com/i/38148927.jpg" />
  <media:content url="https://images.theabcdn.com/i/38148927.jpg" type="image/jpg" medium="image" />
  <itunes:duration>6163</itunes:duration>
