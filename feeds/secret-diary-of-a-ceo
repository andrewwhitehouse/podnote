  <link>https://audioboom.com/posts/7827126</link>
  <enclosure url="https://audioboom.com/posts/7827126.mp3?modified=1617701295&amp;source=rss&amp;stitched=1" length="94631495" type="audio/mpeg" />
  <media:content url="https://audioboom.com/posts/7827126.mp3?modified=1617701295&amp;source=rss&amp;stitched=1" type="audio/mpeg" medium="audio" duration="5912" lang="en-GB" fileSize="94631495" />
  <itunes:image href="https://images.theabcdn.com/i/36185238.jpg" />
